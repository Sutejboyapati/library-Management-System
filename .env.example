# Copy this to .env and fill in your own values.
# Do not commit .env to git.

# Your MySQL connection (use your computer's MySQL user and password)
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=
DB_NAME=library_db

# JWT secret for login tokens (use a long random string in production)
JWT_SECRET=your-secret-key-change-in-production

# Port the API runs on (frontend proxy expects 3000)
PORT=3000
